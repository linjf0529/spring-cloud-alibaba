<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linjf.cloud.dao.OrderMapper">

    <select id="findById" resultType="com.linjf.cloud.entity.Order">
        select id,user_id as userId,name,price,num from tb_order where id=#{id}
    </select>
</mapper>
